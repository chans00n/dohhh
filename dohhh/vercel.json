{
  "buildCommand": "./vercel-build.sh",
  "outputDirectory": ".medusa/server",
  "installCommand": "yarn install",
  "framework": null,
  "functions": {
    "api/serverless.js": {
      "maxDuration": 30,
      "includeFiles": ".medusa/server/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/serverless.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}