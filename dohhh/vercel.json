{
  "buildCommand": "./vercel-build.sh",
  "outputDirectory": ".",
  "installCommand": "yarn install",
  "framework": null,
  "functions": {
    "api/serverless.js": {
      "maxDuration": 30,
      "includeFiles": ".medusa/server/**,node_modules/**,medusa-config.js"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/serverless.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}