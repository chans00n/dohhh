{
  "framework": null,
  "buildCommand": "cd dohhh && yarn build",
  "outputDirectory": ".",
  "installCommand": "cd dohhh && yarn install --production",
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}