{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "cd dohhh && yarn install && yarn build"
  },
  "deploy": {
    "startCommand": "cd dohhh && NODE_OPTIONS='--dns-result-order=ipv4first' yarn start:railway",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}